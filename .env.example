HF_TOKEN=your_huggingface_token_here
CUDA_VISIBLE_DEVICES=0
ENABLE_TRAINING=true
ENABLE_INFERENCE=true
TRT_ENGINE_PATH=/app/trt/transformer.plan
MODEL_PATH=black-forest-labs/FLUX.1-dev
OUTPUT_DIR=/app/output
MINER_SERVER_PORT=8091
MINER_SERVER_HOST=0.0.0.0

# IMPORTANT: Set this to your public URL in production
# SERVICE_URL=https://your-domain.com:8091
# SERVICE_URL=http://your-ip-address:8091

# For reverse proxy (if using)
# INFERENCE_SERVER_URL=http://localhost:8091

# Kontext Image Editing (optional)
ENABLE_KONTEXT_EDIT=false
KONTEXT_MODEL_PATH=black-forest-labs/FLUX.1-Kontext-dev

# Determinism enforcement
PYTHONHASHSEED=0
CUBLAS_WORKSPACE_CONFIG=:4096:8